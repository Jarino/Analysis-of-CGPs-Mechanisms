#!/bin/bash -login
 
### define resources needed:
### walltime - how long you expect the job to run
#PBS -l walltime=10:00:00
 
### nodes:ppn - how many nodes & cores per node (ppn) that you require
#PBS -l nodes=1
 
### mem: amount of memory that the job will need
#PBS -l mem=4gb
 
### you can give your job a name for easier identification
#PBS -N PROBLEM_SEED
 
### if you are part of the buy in group, please edit the following
### line with your group id, and uncomment the line
## PBS -A <buy_in_account_info>

#PBS -j oe
#PBS -o /mnt/home/goldma72/code/cgp/clusterout/PROBLEM_NODES_MUT_SEED.temp

### load necessary modules, e.g.
module load HDF5

### change to the working directory where your code is located
PATH=$PATH:/mnt/home/goldma72/bin
cd /mnt/home/goldma72/code/cgp

### call your executable
pypy main.py cfg/once.cfg cfg/PROBLEM.cfg -g NODES -m MUT -seed SEED -o output/PROBLEM_NODES_MUT_reorder_SEED.dat -r
pypy main.py cfg/once.cfg cfg/PROBLEM.cfg -g NODES -m MUT -seed SEED -o output/PROBLEM_NODES_MUT_reeval_SEED.dat -s no_reeval
pypy main.py cfg/once.cfg cfg/PROBLEM.cfg -g NODES -m MUT -seed SEED -o output/PROBLEM_NODES_MUT_mutate_SEED.dat -s mutate_until_change
pypy main.py cfg/once.cfg cfg/PROBLEM.cfg -g NODES -m MUT -seed SEED -o output/PROBLEM_NODES_MUT_popsize_SEED.dat -pop_size 10
pypy main.py cfg/once.cfg cfg/PROBLEM.cfg -g NODES -m MUT -seed SEED -o output/PROBLEM_NODES_MUT_normal_SEED.dat
